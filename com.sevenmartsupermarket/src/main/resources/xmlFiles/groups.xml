<suite name="Grouping">
	<listeners>
		<listener
			class-name="com.sevenmartsupermarket.listeners.ExtentReporter">
		</listener>
	</listeners>
	<test name="TestByGroup">
	<groups>
	<run>
	<include name ="smoke"></include>
	<include name ="regression"></include>
	</run>
	</groups>
		<classes>
			<class name="com.sevenmartsupermarket.tests.HomeTest">
			</class>
			<class name="com.sevenmartsupermarket.tests.LoginTest"></class>
			<class name="com.sevenmartsupermarket.tests.AdminUserTest"></class>
		</classes>
	</test>
	
</suite>